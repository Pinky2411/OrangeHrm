\tab public void searchEmployee(String name, String job_title, String location) \{\par
\tab\tab boolean isNameValid = name!=null && !name.isBlank() && !name.isEmpty();\par
\tab\tab boolean isJobTitleValid = job_title!=null&& !job_title.isBlank() && !job_title.isEmpty();\par
\tab\tab boolean isLocationValid = location!=null&& !location.isBlank() && !location.isEmpty();\par
\par
\tab\tab try \{\par
\tab\tab\tab // three conditions criteria should be kept top in the order\par
\tab\tab\tab if(isNameValid && isJobTitleValid && isLocationValid) \{\par
\tab\tab\tab\tab Thread.sleep(2000);\par
\tab\tab\tab\tab clickOnDirectory();\par
\tab\tab\tab     Thread.sleep(2000);\par
\tab\tab\tab     selectEmployeeName(name);\par
\tab\tab\tab     Thread.sleep(2000);\par
\tab\tab\tab     selectJobTitle(job_title);\par
\tab\tab\tab     Thread.sleep(2000);\par
\tab\tab\tab     selectLocation(location);\par
\tab\tab\tab     Thread.sleep(2000);\par
\tab\tab\tab     clickOnSearchButton();\par
\tab\tab\tab     Thread.sleep(2000);\par
\tab\tab\tab\}\par
\par
\tab\tab\tab //two conditions criteria should be kept next in the order\par
\tab\tab\tab else if(isNameValid && isJobTitleValid ) \{\par
\tab\tab\tab\tab Thread.sleep(2000);\par
\tab\tab\tab\tab clickOnDirectory();\par
\tab\tab\tab     Thread.sleep(2000);\par
\tab\tab\tab     selectEmployeeName(name);\par
\tab\tab\tab     Thread.sleep(2000);\par
\tab\tab\tab     selectJobTitle(job_title);\par
\tab\tab\tab     Thread.sleep(2000);\par
\tab\tab\tab     clickOnSearchButton();\par
\tab\tab\tab     Thread.sleep(2000);\par
\tab\tab\tab\}\par
\par
\tab\tab\tab //one condition criteria should be kept next in the order\par
\tab\tab\tab else if(isNameValid) \{\par
\tab\tab\tab\tab Thread.sleep(2000);\par
\tab\tab\tab\tab clickOnDirectory();\par
\tab\tab\tab     Thread.sleep(2000);\par
\tab\tab\tab     selectEmployeeName(name);\par
\tab\tab\tab     Thread.sleep(2000);\par
\tab\tab\tab     clickOnSearchButton();\par
\tab\tab\tab     Thread.sleep(2000);\par
\tab\tab\tab\}\par
\tab\tab\tab else if (isJobTitleValid) \{\par
\tab\tab\tab\tab Thread.sleep(2000);\par
\tab\tab\tab\tab clickOnDirectory();\par
\tab\tab\tab     Thread.sleep(2000);\par
\tab\tab\tab     selectJobTitle(job_title);\par
\tab\tab\tab     Thread.sleep(2000);\par
\tab\tab\tab     clickOnSearchButton();\par
\tab\tab\tab     Thread.sleep(2000);\par
\tab\tab\tab\}\par
\tab\tab\tab else if(isLocationValid) \{\par
\tab\tab\tab\tab    Thread.sleep(2000);\par
\tab\tab\tab\tab    clickOnDirectory();\par
\tab\tab\tab        Thread.sleep(2000);\par
\tab\tab\tab        selectLocation(location);\par
\tab\tab\tab        Thread.sleep(2000);\par
\tab\tab\tab        clickOnSearchButton();\par
\tab\tab\tab        Thread.sleep(2000);\par
\tab\tab\tab\}\par
\tab\tab\tab else\{\par
\tab\tab\tab     System.out.println("Invalid Input");\par
\tab\tab\tab\}\par
\tab\tab\}\par
\tab\tab catch(Exception e) \{\par
\tab\tab\tab System.out.println(e.getMessage());\par
\tab\tab\}\par
\tab\}\par
\par
\par
\tab public void clickOnDirectory()\{\par
\tab\tab //Getting input from properties file\par
\tab\tab Properties p = loadProperty();\par
\tab\tab String directory_locator = p.getProperty("directory_locator");\par
\tab\tab //ClickOn Directory\par
\tab\tab driver.findElement(By.xpath(directory_locator)).click();\par
\tab\}\par
\par
\tab public void selectEmployeeName(String name) \{\par
\tab\tab //Getting input from properties file\par
\tab\tab Properties p = loadProperty();\par
\tab\tab String employeename_locator = p.getProperty("directory_employeename_locator");\par
\tab\tab String employeename_list_locator = p.getProperty("directory_employeename_list_locator");\par
\tab\tab //Select Employee Name\par
\tab\tab WebElement employee = driver.findElement(By.xpath(employeename_locator));\par
\tab\tab employee.click();\par
\tab\tab employee.sendKeys(name);\par
        //Select Employee from Employee List\par
\tab\tab List <WebElement> employee_list = driver.findElements(By.xpath(employeename_list_locator));\par
\tab\tab for(WebElement employeename : employee_list) \{\par
\tab\tab\tab if(employeename.getText().startsWith(name)) \{\par
\tab\tab\tab\tab employeename.click();\par
\tab\tab\tab\tab break;\par
\tab\tab\tab\}\par
\tab\tab\}\par
\tab\}\par
\par
\tab public void clickOnSearchButton() \{\par
\tab\tab //Getting input from properties file\par
\tab\tab Properties p = loadProperty();\par
\tab\tab String search_button_locator = p.getProperty("directory_search_button_locator");\par
\tab\tab //ClickOn Search Button\par
\tab\tab driver.findElement(By.xpath(search_button_locator)).click();\par
\tab\}\par
\par
\tab public void selectJobTitle(String job_title) \{\par
\tab\tab //Getting input from properties file\par
\tab\tab Properties p = loadProperty();\par
\tab\tab String jobtitle_locator = p.getProperty("directory_jobtitle_locator");\par
\tab\tab String jobtitle_list_locator = p.getProperty("directory_jobtitle_list_locator");\par
\tab     //Select JobTitle\par
\tab\tab driver.findElement(By.xpath(jobtitle_locator)).click();\par
\tab     //Select JobTitle from List\par
\tab\tab List <WebElement> jobtitle_list = driver.findElements(By.xpath(jobtitle_list_locator));\par
\tab   \tab\tab for(WebElement jobtitle : jobtitle_list) \{\par
\tab   \tab\tab\tab if(jobtitle.getText().equalsIgnoreCase(job_title)) \{\par
\tab   \tab\tab\tab\tab jobtitle.click();\par
\tab   \tab\tab\tab\tab break;\par
\tab   \tab\tab\tab\}\par
\tab   \tab\tab\}\par
   \}\par
\par
\tab public void selectLocation(String location) \{\par
\tab\tab //Getting input from properties file\par
\tab\tab Properties p = loadProperty();\par
\tab\tab String location_locator = p.getProperty("directory_location_locator");\par
\tab\tab String location_list_locator = p.getProperty("directory_location_list_locator");\par
\tab\tab //Select Location\par
\tab\tab driver.findElement(By.xpath(location_locator)).click();\par
\tab\tab List <WebElement> location_list = driver.findElements(By.xpath(location_list_locator));\par
  \tab\tab for(WebElement location_val : location_list) \{\par
  \tab\tab\tab if(location_val.getText().equalsIgnoreCase(location)) \{\par
  \tab\tab\tab\tab location_val.click();\par
  \tab\tab\tab\tab break;\par
  \tab\tab\tab\}\par
  \tab\tab\}\par
\tab\}\par
\}\par
}
